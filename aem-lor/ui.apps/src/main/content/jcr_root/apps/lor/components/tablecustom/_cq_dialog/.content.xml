<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Accordion custom"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}false">
                <items jcr:primaryType="nt:unstructured">
                    <multifieldwithmap
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Multifieldwithmap"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="true">
                        <items jcr:primaryType="nt:unstructured">
                            <titleone
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Column Title One"
                                name="./titleone"/>
                            <titletwo
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Column Title Two"
                                name="./titletwo"/>
                            <titlethree
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Column Title Three"
                                name="./titlethree"/>
                            <titlefour
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                fieldLabel="Column Title Four"
                                name="./titlefour"/>
                            <well
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/well"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <tabledetailswithmap
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                        composite="{Boolean}true"
                                        fieldDescription="Table Details"
                                        fieldLabel="Table Details"
                                        required="{Boolean}false">
                                        <field
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/container"
                                            emptyText="Table Details"
                                            name="./tabledetailswithmap">
                                            <items jcr:primaryType="nt:unstructured">
                                                <cellone
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    emptyText="cell one"
                                                    fieldLabel="cell one"
                                                    name="./cellone"/>
                                                <celltwo
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    emptyText="cell two"
                                                    fieldLabel="cell two"
                                                    name="./celltwo"/>
                                                <cellthree
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    emptyText="cell three"
                                                    fieldLabel="cell three"
                                                    name="./cellthree"/>
                                                <cellfour
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    emptyText="cell four"
                                                    fieldLabel="cell four"
                                                    name="./cellfour"/>
                                            </items>
                                        </field>
                                    </tabledetailswithmap>
                                </items>
                            </well>
                        </items>
                    </multifieldwithmap>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
